@inherits EngramaComponent


<MudGrid>

	<MudItem sm="12">
		@if (MostrarFuncionalidades.False())
		{
			<MudGrid>
				@foreach (var item in Data.LstPlanTrabajos)
				{
					<MudItem sm="4">
						<CardPlanes PlanTrabajo="item" OnPlanTrabajoSelected="OnPlanTrabajoSelected" MostrarSeleccionar="true"></CardPlanes>
					</MudItem>
				}
			</MudGrid>
		}
	</MudItem>

</MudGrid>
